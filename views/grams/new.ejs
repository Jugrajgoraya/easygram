<%- include('../partials/header') %>
<body>
  <h1>New gram</h1>
  <form method="POST" action="/grams">
    
    <div>
      <label for="srcUrl">srcUrl</label>
      <input type="text" name="srcUrl" >
    </div>
    <div>
      <label for="description">description</label>
      <input type="textarea" name="description" required>
